{
  "name": "base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=true next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint:css": "stylelint './**/*.js'",
    "pretest:e2e:run": "npm run build",
    "start": "next start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "NODE_ENV=test jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage --setupFiles dotenv/config",
    "test:debug": "NODE_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch --setupFiles dotenv/config",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:dev": "NODE_ENV=development start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:run": "NODE_ENV=test start-server-and-test start http://localhost:3000 cy:run",
    "test:watch": "NODE_ENV=test jest --runInBand --watch --setupFiles dotenv/config"
  },
  "dependencies": {
    "apisauce": "^3.0.1",
    "next": "^15.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "cypress": "^14.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-test-renderer": "^18.3.1",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
